		accum 0 abort_if_bitset 2
		accum 0 bitset 2
		playsound blank
		trigger counter1 reset_d1
	}

	inspectsoundstart
	{
		accum 0 abort_if_bitset 2
		accum 0 bitset 2
		playsound blank
		trigger counter1 reset_d1
	}

	inspectbodystart
	{
		accum 0 abort_if_bitset 2
		accum 0 bitset 2
		playsound blank
		trigger counter1 reset_d1
	}

